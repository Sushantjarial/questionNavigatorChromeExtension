{
  "manifest_version": 3,
  "name": "ChatGPT Question Navigator",
  "version": "1.0",
  "description": "Navigate through questions asked in ChatGPT conversations",
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["https://chatgpt.com/*","https://chat.openai.com/*"],
    "js": ["content.js"],
    "css": ["styles.css"]
  }]
}